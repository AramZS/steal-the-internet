    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Bullrich/reveal-eleventy-template/issues"
    },
    "homepage": "https://aramzs.github.io/steal-the-internet/",
    "dependencies": {
        "dotenv": "^16.4.5",
        "markdown-it": "^14.0.0",
        "markdown-it-link-attributes": "^4.0.1",
        "reveal.js": "^4.6.1"
    },
    "main": ".eleventy.js",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
        "prettier": "^3.0.3"
    }
